{
  "name": "@jerrythomas/sentry",
  "version": "0.1.0-beta.7",
  "description": "Drop in authentication for SvelteKit apps using popular auth kits",
  "type": "module",
  "svelte": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "uvu spec",
    "coverage": "c8 --include=src uvu spec",
    "release": "release-it --preRelease=beta --ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrythomas/sentry.git"
  },
  "exports": {
    "./package.json": "./package.json",
    "./.npmignore": "./.npmignore",
    "./global.css": "./global.css",
    "./sentry.svg": "./sentry.svg",
    ".": {
      "import": "./src/index.js"
    },
    "./firebase": {
      "import": "./src/firebase.js"
    },
    "./utils": {
      "import": "./src/utils.js"
    },
    "./shared": {
      "import": "./src/shared.js"
    }
  },
  "keywords": [
    "svelte",
    "auth",
    "firebase",
    "supabase",
    "amplify",
    "auth0",
    "sentry",
    "guard"
  ],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jerrythomas/sentry/issues"
  },
  "homepage": "https://github.com/jerrythomas/sentry#readme",
  "devDependencies": {
    "@firebase/app-types": "0.6.2",
    "@sveltejs/kit": "^1.0.0-next.123",
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^10.2.6",
    "c8": "^7.7.3",
    "firebase": "^9.0.0-beta.5",
    "postcss": "^8.3.5",
    "release-it": "^14.10.0",
    "semantic-release": "^17.4.4",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^4.7.3",
    "tailwindcss": "^2.2.4",
    "uvu": "^0.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "auto-changelog": {
    "unreleased": false
  }
}
