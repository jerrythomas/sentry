{
  "name": "@jerrythomas/sentry",
  "version": "1.0.0-beta.8",
  "description": "Drop in authentication for SvelteKit apps using popular auth kits",
  "type": "module",
  "svelte": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "uvu spec",
    "coverage": "c8 --include=src uvu spec",
    "release": "release-it --preRelease=beta"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrythomas/sentry.git"
  },
  "exports": {
    "./package.json": "./package.json",
    "./.npmignore": "./.npmignore",
    "./global.css": "./global.css",
    "./sentry.svg": "./sentry.svg",
    ".": {
      "import": "./src/index.js"
    },
    "./firebase": {
      "import": "./src/firebase.js"
    },
    "./utils": {
      "import": "./src/utils.js"
    },
    "./shared": {
      "import": "./src/shared.js"
    }
  },
  "keywords": [
    "svelte",
    "auth",
    "firebase",
    "supabase",
    "amplify",
    "auth0",
    "sentry",
    "guard"
  ],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jerrythomas/sentry/issues"
  },
  "homepage": "https://github.com/jerrythomas/sentry#readme",
  "devDependencies": {
    "@firebase/app-types": "0.6.2",
    "@sveltejs/kit": "^1.0.0-next.156",
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^10.3.2",
    "c8": "^7.8.0",
    "firebase": "^9.0.0-beta.8",
    "miragejs": "^0.1.41",
    "postcss": "^8.3.6",
    "release-it": "^14.11.5",
    "svelte": "^3.42.3",
    "svelte-preprocess": "^4.8.0",
    "tailwindcss": "^2.2.7",
    "uvu": "^0.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "auto-changelog": {
    "unreleased": false
  }
}
