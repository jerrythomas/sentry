{
  "name": "@jerrythomas/sentry",
  "version": "1.0.0-beta.21",
  "description": "Drop in authentication for SvelteKit apps using popular auth kits",
  "type": "module",
  "svelte": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "uvu spec",
    "coverage": "c8 --include=src uvu spec ",
    "coverage:lcov": "c8 --reporter=lcov --include=src uvu spec ",
    "release": "release-it --preRelease=beta",
    "lint": "eslint src spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrythomas/sentry.git"
  },
  "exports": {
    "./package.json": "./package.json",
    "./.npmignore": "./.npmignore",
    "./global.css": "./global.css",
    "./sentry.svg": "./sentry.svg",
    ".": {
      "import": "./src/index.js"
    },
    "./firebase": {
      "import": "./src/firebase.js"
    }
  },
  "keywords": [
    "svelte",
    "auth",
    "firebase",
    "supabase",
    "amplify",
    "auth0",
    "sentry",
    "guard"
  ],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jerrythomas/sentry/issues"
  },
  "homepage": "https://github.com/jerrythomas/sentry#readme",
  "devDependencies": {
    "@firebase/app-types": "0.6.2",
    "@sveltejs/kit": "^1.0.0-next.216",
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^10.4.1",
    "c8": "^7.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fetch-mock": "^9.11.0",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.1",
    "svelte-windicss-preprocess": "^4.2.5",
    "uvu": "^0.5.3",
    "windicss": "^3.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "auto-changelog": {
    "unreleased": false
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.29.1",
    "cookie": "^0.4.1",
    "firebase": "^9.6.1",
    "supabase": "^0.5.0"
  }
}
